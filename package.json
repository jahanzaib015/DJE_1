{
  "name": "ocrd-extractor-frontend",
  "version": "1.0.0",
  "description": "Modern OCRD Extractor with Node.js frontend",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "node server.js",
    "install-all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["ocrd", "extractor", "pdf", "analysis", "nodejs"],
  "author": "OCRD Extractor Team",
  "license": "MIT"
}
